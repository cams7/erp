	<queryString>
		<![CDATA[SELECT C.CODCLI, C.RAZCLI, C.DDDCLI, C.FONECLI,
C.CODVEND, V.NOMEVEND,
(SELECT FIRST 1 P.DATAPED FROM RPPEDIDO P
 WHERE P.CODEMPCL=C.CODEMP AND P.CODFILIALCL=C.CODFILIAL AND P.CODCLI=C.CODCLI
 order BY P.DATAPED DESC) ULTIMA_DATA,
(SELECT FIRST 1 P.CODPED FROM RPPEDIDO P
 WHERE P.CODEMPCL=C.CODEMP AND P.CODFILIALCL=C.CODFILIAL AND P.CODCLI=C.CODCLI
 order BY P.DATAPED DESC) ULTIMO_PEDIDO,
(SELECT FIRST 1 P.VLRLIQPED FROM RPPEDIDO P
 WHERE P.CODEMPCL=C.CODEMP AND P.CODFILIALCL=C.CODFILIAL AND P.CODCLI=C.CODCLI
 order BY P.DATAPED DESC) ULTIMO_VALOR
FROM RPCLIENTE C, RPVENDEDOR V
WHERE C.CODEMP=43 AND C.CODFILIAL=1 AND
V.CODEMP=C.CODEMPVO AND V.CODFILIAL=C.CODFILIALVO AND V.CODVEND=C.CODVEND
ORDER BY C.CODVEND, 7 DESC]]>
