(p.codemp = ms.codemp and p.codfilial = ms.codfilial and p.codprod = ms.codprod)
left join eqtipomov tm on
(tm.codemp = ms.codemp and tm.codfilial = ms.codfilial and tm.codtipomov = ms.codtipomov)
