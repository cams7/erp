(SELECT * FROM EQMOVPROD MV WHERE MV.CODEMPPD=PD.CODEMP AND MV.CODFILIALPD=PD.CODFILIAL AND MV.CODPROD=PD.CODPROD)  AND ATIVOPROD='S' AND TIPOPROD='S'
