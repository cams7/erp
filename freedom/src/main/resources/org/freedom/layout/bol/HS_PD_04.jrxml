	<variable name="CNPJCLIF" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA[( $F{CNPJCLI} != null ?
$F{CNPJCLI}.substring(0,2) + "." + $F{CNPJCLI}.substring(2,5) + "." + $F{CNPJCLI}.substring(5,8) + "/" +
$F{CNPJCLI}.substring(8,12) + "-" + $F{CNPJCLI}.substring(12) : "" )]]></variableExpression>
	</variable>
	<variable name="CPFCLIF" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA[($F{CPFCLI}==null? "" :($F{CPFCLI}.length()<=9 ? $F{CPFCLI} : $F{CPFCLI}.substring(0,9)+$F{CPFCLI}.substring(9)))]]></variableExpression>
	</variable>
	<variable name="CODBAR" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA[((org.freedom.library.business.component.Banco) $V{BOLETO} )
    .geraCodBar( $F{CODBANCO},  $F{CODFBNMOEDA}, $F{DVBANCO},
        new Long($F{FATVENC}.longValue()), $F{VLRPARCITREC},
        $F{CONVCOB}, $P{TPNOSSONUMERO} ,
        new Long($F{DOCREC}.longValue()) ,
        new Long($F{SEQNOSSONUMERO}.longValue()),
        new Long($F{CODREC}.longValue()),
        new Long($F{NPARCITREC}.longValue()),
        null, $F{AGENCIACONTA},
        $F{NUMCONTA}, $F{CODCARTCOB}, $F{MDECOB} )]]></variableExpression>
		<initialValueExpression><![CDATA["12345678901234567890123456789012345678901234"]]></initialValueExpression>
	</variable>
	<variable name="AGENCIACONTA" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA[$F{AGENCIACONTA}.trim()+ "." +$F{MDECOB} + "." + org.freedom.library.business.component.Banco.strZero($F{CONVCOB},8) + "-" + $F{DVCONVCOB}]]></variableExpression>
	</variable>
	<variable name="VCONVENIO" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA[((org.freedom.library.business.component.Banco) $V{BOLETO} ).geraConvenio($F{CONVCOB}, org.freedom.library.business.component.Banco.HSBC)]]></variableExpression>
	</variable>
	<variable name="NOSSONUMERO" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA[((org.freedom.library.business.component.Banco) $V{BOLETO} )
    .geraNossoNumero($P{TPNOSSONUMERO}, $F{MDECOB},
        $V{VCONVENIO}, new Long($F{DOCREC}.longValue()),
        new Long($F{SEQNOSSONUMERO}.longValue()),
        new Long($F{CODREC}.longValue()),
        new Long($F{NPARCITREC}.longValue()),
        $F{DTVENCITREC}, true, false )]]></variableExpression>
	</variable>
	<variable name="LINHADIG" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA[((org.freedom.library.business.component.Hsbc) $V{BOLETO} ).
    geraLinhaDig( $V{CODBAR},
        new Long(String.valueOf($F{FATVENC})),
        $F{DESCPONT}.equals("S")?$F{VLRPARCITREC}:$F{VLRAPAGITREC} )]]></variableExpression>
		<initialValueExpression><![CDATA["012345678901234567890123456789012345678901234567"]]></initialValueExpression>
	</variable>
	<variable name="DOCUMENTO" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA[((org.freedom.library.business.component.Banco) $V{BOLETO} ).getNumCli($P{TPNOSSONUMERO}, $F{MDECOB}, $V{VCONVENIO}, new Long(String.valueOf($F{DOCREC})), new Long(String.valueOf($F{SEQNOSSONUMERO})), new Long(String.valueOf($F{CODREC})), new Long(String.valueOf($F{NPARCITREC})) )]]></variableExpression>
	</variable>
	<variable name="BOLETO" class="java.lang.Object" resetType="Page">
		<variableExpression><![CDATA[new org.freedom.library.business.component.Hsbc(
    $F{CODBANCO},  $F{CODFBNMOEDA}, $F{DVBANCO},
    new Long(String.valueOf($F{FATVENC})),
    $F{DESCPONT}.equals("S")?$F{VLRPARCITREC}:$F{VLRAPAGITREC},
    $F{CONVCOB}, $P{TPNOSSONUMERO}, new Long(String.valueOf($F{DOCREC})),
    new Long(String.valueOf($F{SEQNOSSONUMERO})),
    new Long(String.valueOf($F{CODREC})),
    new Long(String.valueOf($F{NPARCITREC})),
    null, $F{AGENCIACONTA},
    $F{NUMCONTA}, $F{CODCARTCOB}, $F{MDECOB} )]]></variableExpression>
		<initialValueExpression><![CDATA[new org.freedom.library.business.component.Hsbc()]]></initialValueExpression>
	</variable>
	<variable name="LINHADIGEXT" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA[$V{LINHADIG}]]></variableExpression>
		<initialValueExpression><![CDATA["012345678901234567890123456789012345678901234567"]]></initialValueExpression>
	</variable>
	<variable name="SIGLACARTEIRA" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA[((org.freedom.library.business.component.Banco) $V{BOLETO} ).getSiglaCarteiraBanco()]]></variableExpression>
	</variable>
	<variable name="DOCUMENTOIMP" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA["S".equals($P{IMPDOCBOL})?String.valueOf($F{DOCREC})+"/"+String.valueOf($F{NPARCITREC}):$V{DOCUMENTO}]]></variableExpression>
	</variable>
	<variable name="DIGVERIFNOSSONUMERO" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA[($F{CODCARTCOB} != null && $V{NOSSONUMERO} != null
    ?((org.freedom.library.business.component.Banco) $V{BOLETO} )
    .digVerif( $V{NOSSONUMERO} )
    : "")]]></variableExpression>
	</variable>
