	</variable>
	<variable name="BOLETO" class="java.lang.Object" resetType="Page">
		<variableExpression><![CDATA[new org.freedom.library.business.component.CaixaEconomica( $F{CODBANCO},  $F{CODFBNMOEDA}, $F{DVBANCO}, new Long(String.valueOf($F{FATVENC})), $F{DESCPONT}.equals("S")?$F{VLRPARCITREC}:$F{VLRAPAGITREC},  $F{CONVCOB}, $P{TPNOSSONUMERO}, new Long(String.valueOf($F{DOCREC})), new Long(String.valueOf($F{SEQNOSSONUMERO})), new Long(String.valueOf($F{CODREC})), new Long(String.valueOf($F{NPARCITREC})), null, $F{AGENCIACONTA}, $F{NUMCONTA}, $F{CODCARTCOB}, $F{MDECOB} )]]></variableExpression>
		<initialValueExpression><![CDATA[new org.freedom.library.business.component.CaixaEconomica( $F{CODBANCO},  $F{CODFBNMOEDA}, $F{DVBANCO}, new Long(String.valueOf($F{FATVENC})), $F{DESCPONT}.equals("S")?$F{VLRPARCITREC}:$F{VLRAPAGITREC},  $F{CONVCOB}, $P{TPNOSSONUMERO}, new Long(String.valueOf($F{DOCREC})), new Long(String.valueOf($F{SEQNOSSONUMERO})), new Long(String.valueOf($F{CODREC})), new Long(String.valueOf($F{NPARCITREC})), null, $F{AGENCIACONTA}, $F{NUMCONTA}, $F{CODCARTCOB}, $F{MDECOB} )]]></initialValueExpression>
	</variable>
	<variable name="LINHADIGEXT" class="java.lang.String" resetType="Page">
		<variableExpression><![CDATA[$V{LINHADIG}.length()>=47
?$V{LINHADIG}.substring(0,5)+"."+$V{LINHADIG}.substring(5,10)+" "+
$V{LINHADIG}.substring(10,15)+"."+$V{LINHADIG}.substring(15,21)+" "+
$V{LINHADIG}.substring(21,26)+"."+$V{LINHADIG}.substring(26,32)+" "+
$V{LINHADIG}.substring(32,33)+" "+$V{LINHADIG}.substring(33)
:$V{LINHADIG}]]></variableExpression>
		<initialValueExpression><![CDATA["012345678901234567890123456789012345678901234567"]]></initialValueExpression>
	</variable>
