	<queryString>
		<![CDATA[select ip.codemp, ip.codfilial, ip.dtvencitpag vencimento,
cast(sum(ip.vlrapagitpag) as numeric(15,2)) vlrapagar, cast(0 as decimal(15,2)) vlrreceber
from fnitpagar ip
where
ip.codemp = 71 and ip.codfilial = 1 and ip.dtvencitpag between '2010-03-01' and '2010-03-31'
and ip.statusitpag != 'PP' and cast(ip.vlrapagitpag as numeric(15,2)) > 0
group by 1 , 2, 3

union all

select ir.codemp, ir.codfilial, coalesce(ir.dtprevitrec,ir.dtvencitrec) vencimento,
cast(0 as decimal(15,2)) vlrpagar,
cast(sum(ir.vlrapagitrec) as numeric(15,2)) vlrareceber
from fnitreceber ir
where
ir.codemp = 71 and ir.codfilial = 1 and ir.dtvencitrec between '2010-03-01' and '2010-03-31'
and ir.statusitrec != 'RP' and cast(ir.vlrapagitrec as numeric(15,2)) > 0

group by 1 , 2, 3

order by 1, 3, 3]]>
	</queryString>
