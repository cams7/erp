	<queryString>
		<![CDATA[select rm.codfor, fr.nomefor, tr.nometran, rm.placaveiculo, rm.dtent, rm.desconto,
coalesce(br.nomebairro,'') nomebairro, rm.ticket, br.vlrfrete preco,
(select first 1 coalesce(ra.pesoamost,0) from eqrecamostragem ra where ra.codemp=rm.codemp and
ra.codfilial=rm.codfilial and rm.ticket=ra.ticket order by ra.coditrecmerc, ra.codamostragem )
- (select first 1 coalesce(ra.pesoamost, 0) from eqrecamostragem ra where ra.codemp=rm.codemp
and ra.codfilial=rm.codfilial and rm.ticket=ra.ticket order by ra.coditrecmerc desc,
ra.codamostragem desc ) peso, rm.mediaamostragem
from eqrecmerc rm
left outer join vdtransp tr on tr.codemp=rm.codemptn and tr.codfilial=rm.codfilialtn and
tr.codtran=rm.codtran
left outer join cpforneced fr on fr.codemp=rm.codempfr and fr.codfilial=rm.codfilialfr and
fr.codfor=rm.codfor
left outer join sgbairro br on br.codpais=rm.codpais and br.siglauf=rm.siglauf and
br.codmunic=rm.codmunic and br.codbairro=rm.codbairro
where rm.codemp=70 and rm.codfilial=1 and rm.dtent between '2013-08-07' and '2013-08-07'
order by rm.dtent,rm.ticket]]>
	</queryString>
